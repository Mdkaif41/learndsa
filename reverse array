#include<iostream>
using namespace std;
void reversearray(int n,int arr[]){
    int start=0;
    int end=n-1;
    while(start <end){
        swap(arr[start],arr[end]);
        start++;
        end--;
    }
}
int main(){
    int arr[5]={41 ,45 ,67 ,34 ,90};
    int n=5;
    reversearray(n,arr);
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    
    return 0;
}
