#include<iostream>
#include <climits>
using namespace std;
int main(){
    int size=5;
    int smallest = INT_MAX; 
    int arr[size]={5 ,6 ,7 ,-9 ,-45};
    
    for(int i=0;i<size;i++){
        smallest =min(arr[i],smallest);
    }
     cout << "THE SMALLEST NO =" << smallest<<endl;
    
    return 0;
}
