#include<iostream>
#include <climits>
using namespace std;
int main(){
    int size=5;
    int largest = INT_MIN; 
    int arr[size]={5 ,6 ,7 ,9 ,45};
    
    for(int i=0;i<size;i++){
        largest =max(arr[i],largest);
    }
     cout << "THE LARGEST NO =" << largest<<endl;
    
    return 0;
}
